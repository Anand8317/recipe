<h1>My Inventories</h1>

<% @inventories.each do |inventory| %>
  <div>
    <%= link_to inventory.name, inventory %>
    <%= link_to "Delete", inventory, method: :delete, data: { confirm: "Are you sure you want to delete this inventory?" } %>
  </div>
<% end %>

<%= link_to "New Inventory", new_inventory_path %>